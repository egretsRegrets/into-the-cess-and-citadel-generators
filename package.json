{
  "name": "into-the-cess-and-citadel-generators",
  "version": "1.0.0",
  "description": "On-the-fly generator tools for/from _Into the Cess and Citadel_ by Alex Coggon and Charlie Ferguson-Avery.",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/app.js",
    "compile": "tsc && tsc-alias",
    "dev": "tsc && (tsc -w & tsc-alias -w) & node --watch ./dist/app.js",
    "format": "prettier --write \"src/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egretsRegrets/into-the-cess-and-citadel-generators.git"
  },
  "keywords": [],
