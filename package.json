  "bugs": {
    "url": "https://github.com/egretsRegrets/into-the-cess-and-citadel-generators/issues"
  },
  "homepage": "https://github.com/egretsRegrets/into-the-cess-and-citadel-generators#readme",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "prettier": "^3.5.3",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  }
}
